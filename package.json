{
  "name": "@zkencrypt/sdk",
  "version": "1.0.0",
  "description": "Official JavaScript/TypeScript SDK for ZKEncrypt Network",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "zkencrypt",
    "fhe",
    "homomorphic-encryption",
    "privacy",
    "solana",
    "blockchain",
    "x402",
    "micropayments",
    "ai-oracle",
    "web3"
  ],
  "author": "ZKEncrypt AI Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/ZKEncrypt-AI/zkencrypt-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/ZKEncrypt-AI/zkencrypt-sdk/issues"
  },
  "homepage": "https://zk-labs.network",
  "dependencies": {
    "@solana/web3.js": "^1.95.8",
    "@solana/wallet-adapter-base": "^0.9.23",
    "@solana/wallet-adapter-phantom": "^0.9.24",
    "@solana/wallet-adapter-solflare": "^0.6.28",
    "bs58": "^5.0.0",
    "tweetnacl": "^1.0.3",
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.9",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
